import java.util.*;
public class Solution{
    public static void main(String[] args){
        int n=8;
        System.out.println("the factors of "+n+"are:");
        Divisor(n);
    }
    static void Divisor(int n){
        for(int i=1;i<=n;i++){
            if(n%i==0){
                System.out.println(i+"");
            }
        }
    }
}