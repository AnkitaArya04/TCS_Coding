import java.util.*;
public class Solution{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int res=1;
        while(n>0){
            int lastDigit=n%10;
            res*=lastDigit;
            n=n/10;

        }
        System.out.println(res);
        }
    }