import java.util.*;
public class main{
    public static void main(String[] args){
        int n=153;
        int sum=0;
        int r;
        int temp=n;
        while(n>0){
            r=n%10;
            n=n/10;
            sum=sum+r*r*r;
        }
        if(temp==sum){
            System.out.println("Armstrong");
        }else{
            System.out.println("not an armstrong");
        }
    }
}