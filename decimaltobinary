import java.util.*;

public class Solution{
    public static void main(String[] args){
        int n=28;
        int[] binary=new int[32];
        int i=0;
        while(n>0){
            binary[i]=n%2;
            i++;
            n=n/2;
        }
        for(int j=i-1;j>=0;j--){
            System.out.println(binary[j]);
        }
        
    }
}