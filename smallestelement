import java.util.*;
public class TUF{
    public static void main(String args[]){
        int arr[]={2,5,3,1,0};
        Sustem.out.println("The smallest element in an array :"+smallestElement);
    }
    static int(int[] arr){
        int min=arr[0];
        for(int i=0;i<arr.length;i++){
            if(arr[i]<min){
                min=arr[i];
            }
        }
        return min;
    }
}