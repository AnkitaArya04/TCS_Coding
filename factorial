import java.util.*;
public class Solution{
    public int factorial(int n){
        int ans=1;
        fo(int i=1;i<=n;i++){
            ans=ans*i;
        }
        return ans;
    }
    public static void main(String[] args){
        int n=5;
        int res=factorial(n);
        System.out.println(res);

    }
}