import java.util.*;

public class Solution{
    public static void main(String[] args ){
        int n=245;
        System.out.println(Reverse(n));
    }
    static private int Reverse(int n){
        int d;
        int rev=0;
        while(n!=0){
            d=n%10;
            rev=rev*10+d;
            n=n/10;
        }
        return rev;
    }
}