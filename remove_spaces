import java.util.*;
public class Solution{
    static int remove_spaces(String str){
        int count=0;
        for(int i=0;i<str.length;i++){
            if(str[i]!=' '){
                str[count]=str[i];
            }
        }
        return count;
    }
    public static void main(String[] args ){
        char[] ch="take u forward".toCharArray();
        int count=remove_spaces(str);
        System.out.println(String.valueOf(str).subsequence(0,count));
    }
}