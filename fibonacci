import java.util.*;

public class Solution{
    public static void main(String[] args){
        int N=5;
        System.out.println(fibonacci(N));
    }
    static int fibonacci(int N){
        if(N<=0){
            return N;
        }
        int last=fibonacci(N-1);
        int slast=fibonacci(N-2);
        return last+slast;
    }
}