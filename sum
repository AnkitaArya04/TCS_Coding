import java.util.*;

public class Solution{
    public static void main(String[] args){
        int n=5;
        int arr[]={1,2,4,5,3};
        int sum=0;
        for(int i=0;i<n;i++){
            sum=sum+arr[i];
        }
        System.out.println("The sum of elements of an array is:"+sum);
    }
}